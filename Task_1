T = readtable("fileExcel.xlsx");
leaders = string(T.x___(T.x____=="Руководитель"));
executors = string(T.x___(T.x____ == "Исполнитель"));
Roles = ["Руководители"; "Исполнители"];
People = [sprintf("%s; ", leaders); sprintf("%s; ", executors)];
Tab = table(Roles, People);
writetable(Tab, "outFile.xlsx")
